package com.felight.java.myapp;

public class BestCase {
	
	public static void generateNumbers(int n){
		
		int[] array = new int[n];
		
		  for(int i=0; i < array.length; i++){
			  array[i] = i+1;
			  System.out.println(array[i]);
		  }	
	}        
}

public class AverageCase {
	
	public static void generateRandomNumbers(int n){
		
		int[] array = new int[n];
		  
      for(int i=0; i < array.length; i++){
			  array[i] = (int) (Math.random()*10000);
			  System.out.println(array[i]);
		  }		
	}
}

public class WorstCase {
	
	public static void generateDescendingNumbers(int n) {
		
		int[] array = new int[n];

		   for(int i=array.length-1; i >= 0; i--) {
			    array[i] = i+1;
			    System.out.println(array[i]);
		  }
	}	
}
